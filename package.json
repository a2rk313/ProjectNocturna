{
  "name": "project-nocturna",
  "version": "2.5.0",
  "type": "commonjs",
  "description": "Full-Stack WebGIS platform for light pollution analysis with PostGIS and GeoServer",
  "main": "js/server.js",
  "scripts": {
    "start": "node js/server.js",
    "dev": "concurrently \"nodemon js/server.js\" \"live-server --port=8081 --watch=js,css,html,data\"",
    "seed": "node -e 'require(\"./js/server.js\").seedDatabase()'"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.0",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet.heat": "^0.2.0",
    "node-fetch": "^3.3.0",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "json-server": "^0.17.4",
    "live-server": "^1.2.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "webgis",
    "light-pollution",
    "postgis",
    "geoserver"
  ],
  "author": "Light Pollution Research Team",
  "license": "MIT"
