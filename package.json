{
  "name": "light-pollution-webgis-platform",
  "version": "2.3.0",
  "type": "module",
  "description": "Dual-mode WebGIS platform for light pollution analysis with real data and collapsible panels",
  "scripts": {
    "start": "live-server --port=3000",
    "dev": "live-server --port=3000 --watch=js,css,html,data,images",
    "proxy": "node proxy-server.js",
    "start:all": "concurrently \"npm run proxy\" \"npm start\""
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.0",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "leaflet-routing-machine": "^3.2.12",
    "leaflet.heat": "^0.2.0",
    "node-fetch": "^3.3.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "json-server": "^0.17.4",
    "live-server": "^1.2.0"
  },
  "keywords": [
    "webgis",
    "light-pollution",
    "astronomy",
    "environmental-science",
    "spatial-analysis",
    "real-data",
    "api-integration"
  ],
  "author": "Light Pollution Research Team",
  "license": "MIT"
}
