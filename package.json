{
  "name": "project-nocturna",
  "version": "1.0.1",
  "description": "Real-time light pollution monitoring platform",
  "main": "server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "refactor-start": "cross-env NODE_ENV=production node server-refactored.js",
    "build": "npm run build:client && npm run build:server",
    "build:server": "babel server.js --out-dir dist --presets env",
    "build:refactored": "babel server-refactored.js --out-dir dist-refactored --presets env",
    "build:client": "webpack --mode production",
    "seed": "node scripts/seed.js",
    "migrate": "node scripts/migrate.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "prettier": "prettier --write .",
    "vercel-dev": "vercel dev",
    "vercel-deploy": "vercel --prod",
    "install-hdf5": "echo 'To enable full HDF5 processing, run: npm install hdf5' && echo 'Note: This requires system dependencies like HDF5 library'",
    "health-check": "node -e \"fetch('http://localhost:3000/health').then(res => res.json()).then(console.log).catch(e => console.error('Health check failed:', e));\"",
    "postinstall": "npm run build:server || echo 'Skipping client build during deployment'",
    "analyze": "source-map-explorer dist/bundle.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.0",
    "helmet": "^8.1.0",
    "h5wasm": "^0.8.11",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.16.0",
    "papaparse": "^5.4.1",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "sharp": "^0.33.4",
    "swagger-ui-express": "^5.0.0",
    "turf": "^3.0.14",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "csv-parser": "^3.0.0",
    "winston": "^3.13.0",
    "xml2js": "^0.6.2",
    "form-data": "^4.0.1",
    "node-fetch": "^3.3.2",
    "qs": "^6.13.1"
  },
  "devDependencies": {
    "@vercel/node": "^3.2.23",
    "nodemon": "^3.1.9",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "source-map-explorer": "^2.5.3",
    "buffer": "^6.0.3",
    "stream-browserify": "^3.0.0",
    "assert": "^2.0.0",
    "stream-http": "^3.2.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "url": "^0.11.0",
    "browserify-zlib": "^0.2.0",
    "util": "^0.12.5",
    "querystring-es3": "^0.2.1",
    "process": "^0.11.10",
    "path-browserify": "^1.0.1",
    "crypto-browserify": "^3.12.0"
  },
  "optionalDependencies": {
    "hdf5": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "light-pollution",
    "environmental-monitoring",
    "geospatial",
    "nasa",
    "viirs",
    "earth-observation",
    "remote-sensing",
    "dark-sky",
    "astronomy"
  ],
  "author": "Project Nocturna Team",
  "license": "MIT"
}
